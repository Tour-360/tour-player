{
  "name": "tour-player",
  "version": "2.28.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tour-360/tour-player"
  },
  "scripts": {
    "build-plugins": "cp -rf ./src/plugins/ ./build/plugins/ && babel ./src/plugins -d ./build/plugins/ --compact",
    "postinstall": "./node_modules/bower/bin/bower i && npm run delete-models && ./node_modules/gulp/bin/gulp.js build",
    "preversion": "node version.js $npm_package_version",
    "postversion": "gulp build && npm run build-plugins && gulp deploy || gulp deploy",
    "start": "node index.js",
    "test": "gulp lint",
    "codestyle": "npx prettier ./src/js/ ./src/plugins/ --write",
    "delete-models": "rm -rf ./bower_components/threejs/examples/models/ && rm -rf ./bower_components/threejs-r97/examples/models/",
    "prepare": "npx husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [
    "tour",
    "virtual",
    "player",
    "360",
    "HTML5",
    "raw"
  ],
  "author": "Maxim Borodin <borodinmk@gmail.com> (http://maximborodin.ru)",
  "contributors": [
    {
      "name": "Maxim Borodin",
      "email": "borodinmk@gmail.com",
      "url": "http://maximborodin.ru"
    },
    {
      "name": "Denis Borodin",
      "email": "denis@tour-360.ru",
      "url": "http://denisborodin.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "bower": "^1.5.3",
    "browser-sync": "^2.10.0",
    "del": "^2.0.2",
    "express": "^4.13.3",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^3.0.2",
    "gulp-cache": "^0.4.6",
    "gulp-concat": "^2.6.0",
    "gulp-imagemin": "^2.3.0",
    "gulp-jscs": "^3.0.1",
    "gulp-jshint": "^1.11.2",
    "gulp-sftp": "^0.1.5",
    "gulp-size": "^1.0.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-stylint": "^3.0.0",
    "gulp-stylus": "^2.1.0",
    "gulp-uglify": "^1.4.1",
    "main-bower-files": "^2.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "husky": "^4.3.8",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  }
}
